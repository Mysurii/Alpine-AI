{
  "name": "Alpine-AI-server",
  "version": "0.0.1",
  "main": "server.js",
  "scripts": {
    "cleanDB": "ts-node ./init/init.ts -r dotenv_config_path=./.env",
    "init": "npm run cleanDB && npm run dev",
    "dev": "nodemon --config nodemon.json ./src/server.ts -L -r dotenv_config_path=./.env",
    "build": "tsc",
    "production": "npm run build && node ./build/server.js",
    "testServer": "ts-node ./src/server.ts -r dotenv_config_path=./.test.env",
    "test": "ts-node ./init/init.ts -r dotenv_config_path=./.test.env && jest --config jest.config.ts --detectOpenHandles",
    "test:ci": "npm run test -- --watchAll=false --ci",
    "gen:keypair": "ts-node ./keypair.ts env_path=./.env",
    "gen:keypair-test": "ts-node ./keypair.ts env_path=./.test.env"
  },
  "engines": {
    "node": "18.x"
  },
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "@types/bcrypt": "^5.0.0",
    "@types/chalk": "^2.2.0",
    "@types/cors": "^2.8.12",
    "@types/express": "^4.17.13",
    "@types/helmet": "^4.0.0",
    "@types/jest": "^27.0.2",
    "@types/jest-dev-server": "^5.0.0",
    "@types/jsonwebtoken": "^8.5.5",
    "@types/node": "^16.10.1",
    "@types/nodemailer": "^6.4.6",
    "@types/superagent": "^4.1.16",
    "@types/supertest": "^2.0.11",
    "@types/validator": "^13.7.1",
    "@types/wait-on": "^5.3.1",
    "@typescript-eslint/eslint-plugin": "^5.42.0",
    "@typescript-eslint/parser": "^5.42.0",
    "eslint": "^8.26.0",
    "jest": "^27.2.2",
    "jest-dev-server": "^6.0.3",
    "jest-junit": "^15.0.0",
    "nodemon": "^2.0.13",
    "supertest": "^6.1.6",
    "ts-jest": "^27.0.5",
    "ts-node": "^10.8.1",
    "typescript": "^4.8.4",
    "wait-on": "^6.0.0"
  },
  "dependencies": {
    "bcrypt": "^5.1.0",
    "chalk": "^4.1.2",
    "cors": "^2.8.5",
    "dotenv": "^15.0.1",
    "express": "^4.17.1",
    "express-rate-limit": "^6.1.0",
    "helmet": "^5.0.1",
    "jsonwebtoken": "^8.5.1",
    "minipass": "^3.2.1",
    "mongodb": "^4.3.1",
    "mongodb-helper": "file:private_node_modules/mongodb-helper-1.0.0.tgz",
    "nodemailer": "^6.8.0",
    "superagent": "^8.0.4",
    "validator": "^13.7.0"
  }
}
